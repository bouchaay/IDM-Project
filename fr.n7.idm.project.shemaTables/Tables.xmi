<?xml version="1.0" encoding="UTF-8"?>
<shemaTables:Tables
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:shemaTables="http://www.example.org/shemaTables">
  <shematables
      name="Tab1"
      id="1">
    <columns
        name="col11"
        id="1"/>
    <columns
        name="col12"
        id="2"/>
    <columns
        xsi:type="shemaTables:ReferencedColumn"
        name="col3"
        id="3"
        foreignTable="//@shematables.1"
        foreignColumn="//@shematables.1/@columns.1"/>
    <columns
        xsi:type="shemaTables:DerivedColumn"
        name="col4"
        id="4"
        outputColumn="//@shematables.0/@catalogue/@algorithm.0/@output"/>
    <catalogue
        name="cat1">
      <algorithm
          name="add"
          path="c://add.py">
        <input
            column="//@shematables.0/@columns.0 //@shematables.0/@columns.1"/>
        <output
            name="col1+col2"/>
      </algorithm>
    </catalogue>
  </shematables>
  <shematables
      name="Tab2"
      id="2">
    <columns
        name="col21"
        id="1"/>
    <columns
        name="col22"
        id="2"/>
    <catalogue
        name="cat2"/>
  </shematables>
</shemaTables:Tables>
