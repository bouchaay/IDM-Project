import 'algorithme.ecore'

package algorithme

context Resource
-- le path des ressources est non null
inv pathNonNul('Invalid path') :
    self.folderPath <> null

context Algorithm
-- Nom de l'algorithme non null
inv nomNonNul('Invalid nom') :
    self.name <> null

context AlgorithmFile
-- Le nom du fichier est non null
inv nomNonNul('Invalid nom') :
    self.fileName <> null

endpackage
